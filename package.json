{
    "name": "numbered-bookmarks",
    "displayName": "Numbered Bookmarks",
    "description": "Mark lines and jump to them, in Delphi style",
    "version": "8.5.0",
    "publisher": "alefragnani",
    "engines": {
        "vscode": "^1.73.0"
    },
    "extensionKind": [
        "ui",
        "workspace"
    ],
    "capabilities": {
        "virtualWorkspaces": true,
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "bookmark",
        "sticky",
        "jump",
        "mark",
        "navigation",
        "Delphi",
        "multi-root ready"
    ],
    "icon": "images/icon.png",
    "license": "GPL-3.0",
    "homepage": "https://github.com/alefragnani/vscode-numbered-bookmarks/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/alefragnani/vscode-numbered-bookmarks.git"
    },
    "bugs": {
        "url": "https://github.com/alefragnani/vscode-numbered-bookmarks/issues"
    },
    "sponsor": {
        "url": "https://github.com/sponsors/alefragnani"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension-node.js",
    "l10n": "./l10n",
    "contributes": {
        "commands": [
            {
