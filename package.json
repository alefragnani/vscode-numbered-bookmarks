            },
            {
                "command": "numberedBookmarks.toggleBookmark9",
                "title": "%numberedBookmarks.commands.toggleBookmark9.title%",
                "category": "Numbered Bookmarks"
            },
            {
                "command": "numberedBookmarks.jumpToBookmark0",
                "title": "%numberedBookmarks.commands.jumpToBookmark0.title%",
                "category": "Numbered Bookmarks"
            },
            {
                "command": "numberedBookmarks.jumpToBookmark1",
                "title": "%numberedBookmarks.commands.jumpToBookmark1.title%",
                "category": "Numbered Bookmarks"
