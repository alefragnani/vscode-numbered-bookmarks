        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "^14.17.27",
        "@types/vscode": "^1.73.0",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "@vscode/test-electron": "^1.6.2",
        "eslint": "^8.1.0",
        "eslint-config-vscode-ext": "^1.1.0",
        "mocha": "^11.1.0",
        "terser-webpack-plugin": "^5.2.4",
        "ts-loader": "^9.2.5",
        "typescript": "^4.4.4",
        "webpack": "^5.94.0",
        "webpack-cli": "^4.8.0"
    }
