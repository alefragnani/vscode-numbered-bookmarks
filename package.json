                "when": "editorTextFocus"
            },
            {
                "command": "numberedBookmarks.toggleBookmark6",
                "key": "ctrl+shift+6",
                "mac": "cmd+shift+6",
                "when": "editorTextFocus"
            },
            {
                "command": "numberedBookmarks.toggleBookmark7",
                "key": "ctrl+shift+7",
